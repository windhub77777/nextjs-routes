{
  "name": "nextjs-routes",
  "version": "0.0.3",
  "description": "Type safe routing for Next.js",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/nextjs-routes"
  },
  "main": "dist/index.js",
  "bin": {
    "nextjs-routes": "dist/cli.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn webpack && yarn package:prune && chmod +x dist/cli.js",
    "clean": "rm -rf dist",
    "lint": "yarn typecheck && prettier --check . && prettier-package-json --list-different '{,example/,}package.json' && eslint .",
    "lint:fix": "prettier --write . && prettier-package-json --write '{,example/}package.json' && eslint --fix .",
    "package:prune": "find dist -name test.* | xargs rm",
    "test": "yarn jest",
    "typecheck": "yarn tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@types/react": "*",
    "next": "*",
    "prettier": "^2.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^4.3.8",
    "jest": "^27.2.4",
    "next": "^12.1.0",
    "prettier": "^2.1.1",
    "prettier-package-json": "^2.1.3",
    "react": "^17.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "keywords": [
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  }
}
